    <servlet-name>main</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>oauth2callback</servlet-name>
    <servlet-class>com.google.glassware.AuthServlet</servlet-class>
  </servlet>
