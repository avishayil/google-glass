  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>LoggingServlet</display-name>
    <servlet-name>LoggingServlet</servlet-name>
    <servlet-class>com.google.glassware.custom.LoggingServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoggingServlet</servlet-name>
    <url-pattern>/LoggingServlet</url-pattern>
  </servlet-mapping>
</web-app>